{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "rootDir": "./js",
    "sourceMap": true,
    "outDir": "./dist/js",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "allowImportingTsExtensions": false,
    "noEmit":false,
    "declaration":true,
    "declarationDir": "./dist/types",
    "composite": true
  }
}
